{
  "name": "get-changed-files",
  "description": "GitHub Action that gets files changed/modified in a pull request or push.",
  "version": "2.4.3",
  "private": true,
  "author": "Jitterbit, Inc.",
  "contributors": [
    "Halas Labs, LLC"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halaslabs/get-changed-files.git"
  },
  "homepage": "https://github.com/halaslabs/get-changed-files#readme",
  "bugs": {
    "url": "https://github.com/halaslabs/get-changed-files/issues"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Get",
    "Changed",
    "Modified",
    "Diff",
    "Files"
  ],
  "main": "lib/main.js",
  "engines": {
    "node": "^20.0.0"
  },
  "scripts": {
    "all": "yarn --ignore-engines clean && yarn --ignore-engines build && yarn --ignore-engines format && yarn --ignore-engines lint && yarn --ignore-engines package",
    "build": "tsc",
    "clean": "rimraf dist/* lib/*",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint src/*.ts",
    "package": "ncc build",
    "test": "jest",
    "watch": "tsc -w -p ."
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "glob-regex": "^0.3.2",
    "is-glob": "^4.0.3"
  },
  "devDependencies": {
    "@octokit/rest": "^21.1.1",
    "@stylistic/eslint-plugin-ts": "^3.1.0",
    "@types/is-glob": "^4.0.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.32.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.25.1",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
