{
  "name": "get-changed-files",
  "description": "GitHub Action that gets files changed/modified in a pull request or push.",
  "version": "2.3.1",
  "private": true,
  "author": "Jitterbit, Inc.",
  "contributors": [
    "Halas Labs, LLC"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halaslabs/get-changed-files.git"
  },
  "homepage": "https://github.com/halaslabs/get-changed-files#readme",
  "bugs": {
    "url": "https://github.com/halaslabs/get-changed-files/issues"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Get",
    "Changed",
    "Modified",
    "Diff",
    "Files"
  ],
  "main": "lib/main.js",
  "engines": {
    "node": "^20.0.0"
  },
  "scripts": {
    "all": "yarn clean && yarn build && yarn format && yarn lint && yarn package",
    "build": "tsc",
    "clean": "rimraf dist/**/* lib/**/*",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "watch": "tsc -w -p ."
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "glob-regex": "^0.3.2",
    "is-glob": "^4.0.3"
  },
  "devDependencies": {
    "@octokit/rest": "^20.0.2",
    "@types/is-glob": "^4.0.2",
    "@types/jest": "^26.0.14",
    "@types/node": "^18.18.4",
    "@typescript-eslint/parser": "^6.10.0",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.51.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
