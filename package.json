{
  "name": "get-changed-files",
  "description": "GitHub Action that gets files changed/modified in a pull request or push.",
  "version": "2.0.0",
  "private": true,
  "author": "Jitterbit, Inc.",
  "contributors": [
    "Halas Labs, LLC"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halaslabs/get-changed-files.git"
  },
  "homepage": "https://github.com/halaslabs/get-changed-files#readme",
  "bugs": {
    "url": "https://github.com/halaslabs/get-changed-files/issues"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Get",
    "Changed",
    "Modified",
    "Diff",
    "Files"
  ],
  "main": "lib/main.js",
  "engines": {
    "node": "^14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/**/* lib/**/*",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "yarn clean && yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@octokit/rest": "^19.0.7",
    "@types/jest": "^25.1.1",
    "@types/node": "^14.14.20",
    "@typescript-eslint/parser": "^2.8.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.6.4"
  }
}
